{
	"root": true,
	"extends": "eslint:recommended",
	"env": {
		"es6": true,
		"browser": true,
		"jasmine": true
	},
	"parserOptions": {
		"ecmaVersion": 2022,
		"sourceType": "module"
	},
	"rules": {
		"no-unused-vars": [
			"error",
			{
				"vars": "local",
				"argsIgnorePattern": "^_"
			}
		],
		"no-console": [
			"error",
			{
				"allow": [
					"warn",
					"error",
					"info",
					"assert"
				]
			}
		],
		"no-warning-comments": "off"
	}
}